      <ImageView class="iconImage" id="iconImage" image="/images/booIcon.png"/>
      <Label class="iconLabel" id="iconLabel"></Label>
    </View>
    <ImageView class="deleteImage" id="deleteImage" image="/images/deleteIcon.png" onClick="deleteDapp" onError="errorLoad"/>
  </View>
</Alloy>
